<html>
<style>
  table,
  th,
  td {
    border: 1px solid black;
  }
</style>

<body>
  <table style="width:100%">
    <tr>
      <th>ID</th>
      <th>Email</th>
      <th>FullName</th>
      <th>Phone</th>
      <th>Address</th>
    </tr>
    <% for(var i=0; i < data.length; i++) { %>
      <tr>
        <td><%= data[i].id %></td>
        <td><%= data[i].email %></td>
        <td><%= data[i].fullName %></td>
        <td><%= data[i].phone %></td>
        <td><%= data[i].address %></td>
      </tr>
    <% } %>
  </table>

</body>

</html>
